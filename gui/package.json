{
  "name": "CalculusToolbox",
  "author": "Samuel Balco",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.49.0"
  },
  "homepage": "./",
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "electron-store": "^1.3.0",
    "katex": "^0.9.0-alpha1",
    "portfinder": "^1.0.13",
    "react": "^16.1.1",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-sizeme": "^2.3.6",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.76.0"
  },
  "main": "public/electron.js",
  "build": {
    "appId": "goodlyrottneapple.CalculusToolbox",
    "mac": {
      "category": "logic.tools.proofAssitant"
    },
    "files": [
      "build/**/*"
    ],
    "extraFiles": [
      {
        "from": "resources",
        "to": "resources/bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron-win": "set ELECTRON_START_URL=http://localhost:3000&& electron .",
    "pack": "electron-builder --dir",
    "dist": "build --em.main=build/electron.js"
  }
}
